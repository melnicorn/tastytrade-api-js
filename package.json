{
  "name": "@slowclap/tastytrade-api",
  "version": "0.0.3",
  "main": "cjs-wrapper.cjs",
  "module": "./dist/tastytrade-api.js",
  "types": "./dist/tastytrade-api.d.ts",
  "exports": {
    "import": {
      "types": "./dist/tastytrade-api.d.ts",
      "import": "./dist/tastytrade-api.js"
    },
    "require": {
      "types": "./dist/tastytrade-api.d.cts",
      "require": "./cjs-wrapper.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "repository": "https://github.com/melnicorn/tastytrade-api",
  "license": "MIT",
  "description": "Typescript impelementation of tastytrade api",
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "jest -i --restoreMocks",
    "unit-test": "node --experimental-vm-modules ./node_modules/.bin/jest tests/unit",
    "integration-test": "node --experimental-vm-modules ./node_modules/.bin/jest tests/integration",
    "lint": "eslint lib/** tests/**",
    "prepublishOnly": "npm run unit-test && npm run build",
    "pack": "rm -rf ./*.tgz && npm run build && npm pack --pack-destination .",
    "postpack": "if [ \"$npm_lifecycle_event\" = \"postpublish\" ]; then git tag v$npm_package_version; fi",
    "postpublish": "git tag -a $npm_package_version -m $npm_package_version && git push origin $npm_package_version"
  },
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "@types/qs": "^6.9.7",
    "axios": "^1.3.4",
    "isomorphic-ws": "^5.0.0",
    "lodash": "^4.17.21",
    "qs": "^6.11.1",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "20.9.0",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.14.0",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "ts-jest": "^29.1.2",
    "tsup": "^8.4.0",
    "typescript": "^5.4.2"
  }
}
